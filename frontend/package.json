{
  "name": "frontend",
  "version": "0.0.1",
  "description": "Notes to email",
  "productName": "Quasar App",
  "author": "Abdullah Gira <abdullah.gira@linq.io>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue ./",
    "format": "prettier --write \"**/*.{js,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "dev": "quasar dev",
    "build": "quasar build",
    "deploy:prod": "npm run build && firebase deploy --only hosting",
    "deploy:nightly": "npm run build && firebase hosting:channel:deploy nightly",
    "deploy:branch": "npm run build && firebase hosting:channel:deploy $(git branch --show-current)"
  },
  "dependencies": {
    "@quasar/extras": "^1.0.0",
    "quasar": "^2.6.0",
    "firebase": "^10.7.1",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "eslint": "^8.10.0",
    "eslint-plugin-vue": "^9.0.0",
    "eslint-config-prettier": "^8.1.0",
    "prettier": "^2.5.1",
    "@quasar/app-vite": "^1.0.0",
    "autoprefixer": "^10.4.2",
    "postcss": "^8.4.14"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
